{
    "name": "sentryxsi/app",
    "description": "App skeleton prototype",
    "type": "project",
    "homepage": "https://github.com/sentryxsi/app",
    "license": "MIT",
    "authors": [
        {
            "name": "SentryXSI"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sentryxsi/rei",
            "no-api" : true
        }
    ],
    "require": {
        "sentryxsi/rei": "0.0.2",
        "php": "^7.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    }
}
